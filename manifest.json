{
    "manifest_version": 3,
    "name": "Smart Bookmarks",
    "version": "3.0",
    "description": " Keep your Bookmarks saved",
    "icons": {
      "48": "icons/icon128.png"
    },
    "permissions": ["storage", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "128": "icons/icon128.png"
      }
    },
    "browser_specific_settings": {
        "gecko": {
          "id": "smartbookmarks@Auraaid.netlify.com",
          "strict_min_version": "58.0"
        }
      }
  }
  
